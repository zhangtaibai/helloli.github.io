{
    "manifest_version": 2,
    "name": "下班路好走",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",
    "content_security_policy": "script-src 'self' https://ss0.bdstatic.com/ https://api.map.baidu.com 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Click here!"
    },
    "permissions": [
        "storage",
        "activeTab",
        "https://api.map.baidu.com/api",
        "https://ss0.bdstatic.com/"
    ]
}